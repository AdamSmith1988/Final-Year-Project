@model IEnumerable<BookingsModel.StaffAccess>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.StaffRole.RoleName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SystemAuthorization.Add_Edit_Del)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsAccessible)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.StaffRole.RoleName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SystemAuthorization.Add_Edit_Del)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsAccessible)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.StaffAccessID }) |
            @Html.ActionLink("Details", "Details", new { id=item.StaffAccessID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.StaffAccessID })
        </td>
    </tr>
}

</table>
